{
	"linkit": {
		"description": "Access group which allows anything",
		"read": {
			"cgi-io": [ "backup", ],
			"ubus": {
				"system": [ "board" ],
				"session": [ "grant", "destroy" ],
				"iwinfo": [ "scan", "info" ],
				"network.interface": [ "status" ],
			},
			"uci": [ "network", "wireless", "system" ]
		},
		"write": {
			"cgi-io": [ "upload", ],
			"file": {
				"/usr/bin/wifi_mode" : [ "exec" ],
			},
			"ubus": {
				"session": [ "grant", "logout" ],
				"uci": [ "get", "set", "rename", "delete", "apply", "reload_config", "commit" ],
				"rpc-sys": [ "*" ],
                		"file": [ "exec" ],
			},
			"uci": [ "network", "wireless", "system" ]
		}
	}
}
